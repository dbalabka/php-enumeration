{
    "name": "dbalabka/php-enumeration",
    "description": "Implementation of Enumeration Classes in PHP. The better alternative for Enums",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Balabka",
            "email": "dmitry.balabka@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "vladimmi/construct-static": "dev-master",
        "myclabs/php-enum": "^1.0",
        "phpunit/phpunit": "^7.5",
        "phpbench/phpbench": "^0.16.9"
    },
    "suggest": {
        "vladimmi/construct-static": "Allows to call __constructStatic on class load"
    },
    "autoload": {
        "psr-4": {
            "Dbalabka\\Enumeration\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dbalabka\\Enumeration\\Examples\\": "examples",
            "Dbalabka\\Enumeration\\Tests\\": "tests"
        }
    }
}
